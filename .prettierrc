{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "proseWrap": "never",
  "importOrder": ["^(react|react-dom)", "^([a-z]|@[a-z])", "", ".*"],
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "overrides": [{ "files": ".prettierrc", "options": { "parser": "json" } }]
}
